<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.calculadora.felipecano.calculadoraprueba">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_filter_1_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_filter_1_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CalculatorActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>
